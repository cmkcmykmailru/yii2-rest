{
  "name": "grigor/yii2-rest",
  "description": "rest full",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "yii2",
    "extension"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "grigor",
      "email": "cmk.cmyk@mail.ru"
    }
  ],
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.x-dev"
  },
  "autoload": {
    "psr-4": {
      "grigor\\rest\\": "src"
    }
  },
  "scripts": {
    "tests": "php ./vendor/bin/phpunit --colors=always tests"
  }
}
